{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "bot",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc",
		"lint": "prettier --check",
		"format": "prettier --write . && eslint --ext .ts --fix --format=pretty src",
		"start": "node --require dotenv/config src/index.ts"
	},
	"dependencies": {
		"@discordjs/core": "^1.1.0",
		"discord.js": "^14.14.0",
		"dotenv": "^16.3.1"
	},
	"devDependencies": {
		"@sapphire/ts-config": "^5.0.0",
		"@types/node": "^18.18.8",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2"
	},
	"packageManager": "yarn@4.0.1"
}
