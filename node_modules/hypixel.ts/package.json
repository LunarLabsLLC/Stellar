{
	"name": "hypixel.ts",
	"version": "1.1.4",
	"description": "A simple, easy to use wrapper for Hypixel API.",
	"homepage": "https://hypixel.js.org/",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"build": "yarn tsc -b src",
		"lint": "yarn eslint src --fix --ext .ts",
		"format": "yarn prettier --write src",
		"test": "yarn vitest",
		"coverage": "yarn test --coverage",
		"prepare": "yarn husky install",
		"prepublishOnly": "yarn build && yarn gen-esm",
		"update": "yarn upgrade-interactive --latest"
	},
	"dependencies": {
		"@discordjs/collection": "^1.5.0",
		"@sapphire/async-queue": "^1.5.0",
		"tslib": "^2.5.0",
		"undici": "^5.21.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.5.1",
		"@commitlint/config-conventional": "^17.4.4",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8.37.0",
		"@types/node": "^18.15.11",
		"@types/prettier": "^2.7.2",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitest/coverage-c8": "^0.29.8",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.7",
		"typedoc": "^0.23.28",
		"typescript": "^4.9.5",
		"vitest": "^0.29.8"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FC5570/hypixel.ts"
	},
	"bugs": {
		"url": "https://github.com/FC5570/hypixel.ts/issues"
	},
	"keywords": [
		"hypixel",
		"hypixel-api",
		"hypixel-api-wrapper",
		"hypixel.js",
		"hypixel.ts"
	],
	"files": [
		"dist/",
		"*.md",
		"!README.md"
	],
	"lint-staged": {
		"*.{ts,js}": "yarn lint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"packageManager": "yarn@3.5.0",
	"prettier": "@sapphire/prettier-config"
}
